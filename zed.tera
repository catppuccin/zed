                    {#- JS & derivative #}
                    "keyword.export": {
                        "color": "#{{ c.sky.hex }}",
                        "font_style": null,
                        "font_weight": null
                    },
                    {#- Punctuation #}
                    "punctuation": {
                        "color": "#{{ c.overlay2.hex }}",
                        "font_style": null,
