                    },
                    "comment.note": {
                        "color": "#{{ c.rosewater.hex }}",
                        "font_style": {{ italics }},
                        "font_weight": null
                    },
                    {#- ! end untested #}
                    {#- Diff #}
                    "diff.plus": {
                        "color": "#{{ c.green.hex }}",
                        "font_style": null,
                        "font_weight": null
                    },
                    "diff.minus": {
                        "color": "#{{ c.red.hex }}",
                        "font_style": null,
