                        "font_style": {{ italics }},
                        "font_weight": null
                    },
                    {#- ! untested, rollback if Zed supports these #}
                    "comment.error": {
                        "color": "#{{ c.red.hex }}",
                        "font_style": {{ italics }},
                        "font_weight": null
                    },
                    "comment.warning": {
                        "color": "#{{ c.yellow.hex }}",
                        "font_style": {{ italics }},
                        "font_weight": null
                    },
                    "comment.hint": {
                        "color": "#{{ c.blue.hex }}",
                        "font_style": {{ italics }},
